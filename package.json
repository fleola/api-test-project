{
  "name": "api-test",
  "version": "1.0.0",
  "description": "first node project",
  "main": "server.js",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "lint": "eslint src/ --cache --fix",
    "build:dev": "tsc --watch",
    "start": "node --enable-source-maps ./dist/server.js",
    "start:dev": "nodemon --watch dist dist/server.js --watch .env",
    "dev": "npm run build && concurrently 'npm:build:dev' 'npm:start:dev'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fleola/api-test-project.git"
  },
  "keywords": [
    "node"
  ],
  "author": "fleola",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fleola/api-test-project/issues"
  },
  "homepage": "https://github.com/fleola/api-test-project#readme",
  "devDependencies": {
    "@tsconfig/node18": "^1.0.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.2",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "concurrently": "^7.3.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^28.1.3",
    "jest-mock-extended": "^2.0.7",
    "nodemon": "^2.0.19",
    "prettier": "2.7.1",
    "prisma": "^4.2.1",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.7",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@prisma/client": "^4.2.1",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1"
  }
}
